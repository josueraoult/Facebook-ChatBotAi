{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
